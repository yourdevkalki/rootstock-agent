{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "lib": ["ES2022"],
    "outDir": "../rootstock-knowledge-graph/dist",
    "rootDir": "../rootstock-knowledge-graph",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true
  },
  "include": [
    "../rootstock-knowledge-graph/*.ts",
    "../rootstock-knowledge-graph/**/*.ts"
  ],
  "exclude": [
    "../rootstock-knowledge-graph/node_modules",
    "../rootstock-knowledge-graph/dist"
  ]
}
